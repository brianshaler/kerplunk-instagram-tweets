{
  "name": "kerplunk-instagram-tweets",
  "displayName": "Instagram + Twitter de-duplication",
  "description": "",
  "version": "0.0.1",
  "kerplunk": {
    "dependencies": [
      "kerplunk-instagram",
      "kerplunk-twitter"
    ],
    "permissions": {
      "kerplunk-activityitem": {
        "models": [
          "ActivityItem"
        ]
      }
    }
  },
  "keywords": [
    "kerplunk-plugin",
    "kp:Social Networking",
    "kp:Instagram",
    "kp:Twitter"
  ],
  "engines": {
    "node": ">=0.10"
  },
  "main": "./lib/index.js",
  "dependencies": {
    "when": "^3.7.3"
  },
  "devDependencies": {
    "coffee-script": "^1.9.1",
    "glut": "0.0.1",
    "gulp": "^3.8.8",
    "gulp-coffee": "^2.3.1"
  },
  "scripts": {
    "prepublish": "rm -rf ./lib ./public && gulp"
  }
}
